<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="BreathView">
        <attr name="bgColor" format="color" />
        <attr name="breathColor" format="color" />
        <attr name="breathTime" format="integer" />
        <attr name="autoRun" format="boolean" />
        <attr name="progress" format="integer" />
        <attr name="maxProgress" format="integer" />
        <attr name="sleepTime">
            <enum name="ST30" value="30" />
            <enum name="ST25" value="25" />
        </attr>
        <attr name="runMode">
            <enum name="START_IN_LEFT_TOP" value="0" />
            <enum name="START_IN_RIGHT_BOTTOM" value="1" />
        </attr>
    </declare-styleable>
</resources>